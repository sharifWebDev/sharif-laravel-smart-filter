{
  "name": "sharifuddin/laravel-smart-filter",
  "description": "Advanced smart filtering for Laravel Eloquent models with relation support and multiple filter types",
  "type": "library",
  "keywords": [
    "laravel",
    "filter",
    "eloquent",
    "smart-filter",
    "relations",
    "query-builder",
    "filtering",
    "laravel-filter",
    "eloquent-filter",
    "advanced-filter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sharif Uddin",
      "email": "sharif.webpro@gmail.com",
      "role": "Developer",
      "homepage": "https://sharifwebdev.github.io/"
    }
  ],
  "version": "1.0.0",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Sharifuddin\\LaravelSmartFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sharifuddin\\LaravelSmartFilter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sharifuddin\\LaravelSmartFilter\\SmartFilterServiceProvider"
      ],
      "aliases": {
        "SmartFilter": "Sharifuddin\\LaravelSmartFilter\\Facades\\SmartFilter"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "test-feature": "vendor/bin/phpunit tests/Feature/",
    "test-unit": "vendor/bin/phpunit tests/Unit/"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
